<?xml version="1.0" encoding="UTF-8" ?>
<!-- 具体映射，用于实现java下的接口（须一一对应），写sql语句的地方 -->
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pbrs.mybatis.mapper.SimilarityMapper">

    <!--Similarity selectSimilarity(@Param("index") int index);-->
    <select id="selectSimilarity" resultType="Similarity">
        select * from max5 where `index` = ${index}
    </select>

</mapper>